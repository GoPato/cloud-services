{
  "private": true,
  "scripts": {
    "postinstall": "flow-typed install --overwrite",
    "dynamodb:install": "sls dynamodb-local install",
    "dynamodb:start": "sls dynamodb-local start --migrate",
    "lint": "eslint .",
    "flow": "flow",
    "flow:watch": "flow-watch",
    "test": "sls dynamodb-jest --runInBand",
    "deploy": "sls deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@gopato/serverless-dynamodb-client": "^1.1.1",
    "@gopato/serverless-dynamodb-local-utils": "^1.1.2",
    "npdynamodb": "^0.2.15",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@gopato/serverless-dynamodb-jest-plugin": "^1.1.1",
    "@gopato/serverless-dynamodb-local-plugin": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "flow-bin": "^0.86.0",
    "flow-typed": "^2.5.1",
    "flow-watch": "^1.1.3",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "pre-commit": "^1.2.2",
    "serverless": "^1.32.0",
    "serverless-appsync-plugin": "^1.0.5",
    "serverless-dotenv-plugin": "^2.0.1",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.25.1"
